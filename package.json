{
  "name": "manhattan",
  "version": "0.9.0",
  "description": "",
  "engines": {
    "node": "8.1.2"
  },
  "main": "./server/server.js",
  "scripts": {
    "clean": "rimraf build",
    "lint": "require-lint",
    "compile": "./node_modules/.bin/babel server --out-dir build",
    "dev": "babel-node ./server/server.js",
    "test": "npm run lint && NODE_ENV=test nyc mocha ./tests/* --compilers js:babel-core/register",
    "build": "babel ./server/server.js -d /build",
    "serve": "nodemon ./build/server/server.js",
    "webpack": "./node_modules/.bin/webpack --progress",
    "start": "NODE_ENV=prod && ./node_modules/.bin/babel-node ./build/server.js"
  },
  "keywords": [
    "node",
    "babel"
  ],
  "author": "Jasdeep Madan",
  "license": "ISC",
  "babel": {
    "presets": [
      "es2015",
      "stage-0",
      "stage-1"
    ]
  },
  "dependencies": {
    "babel-cli": "^6.24.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "bayes": "0.0.6",
    "body-parser": "^1.17.2",
    "brain": "^0.7.0",
    "cheerio": "^1.0.0-rc.1",
    "cron": "^1.2.1",
    "express": "^4.15.3",
    "feedparser": "^2.2.1",
    "htmlparser2": "^3.9.2",
    "mongodb": "^2.2.28",
    "morgan": "^1.8.2",
    "natural": "^0.5.4",
    "nbayes": "^3.0.2",
    "node-cron": "^1.2.0",
    "parse5": "^3.0.2",
    "pg": "7.1.2",
    "request": "^2.81.0",
    "request-promise": "^4.2.1",
    "require-lint": "^1.3.0",
    "restify": "^6.0.0",
    "restify-cors-middleware": "^1.0.1",
    "synaptic": "^1.0.12",
    "textract": "^2.1.2"
  },
  "devDependencies": {
    "babel-loader": "^7.1.1",
    "babel-preset-env": "^1.6.0",
    "chai": "^4.1.1",
    "clean-webpack-plugin": "^0.1.10",
    "css-loader": "^0.28.4",
    "file-loader": "^0.11.2",
    "mocha": "^3.5.3",
    "mocha-as-promised": "^2.0.0",
    "nyc": "^11.1.0",
    "require-lint": "^1.3.0",
    "rimraf": "^2.5.4",
    "sinon": "^3.0.0",
    "style-loader": "^0.18.2",
    "webpack": "^3.3.0",
    "webpack-node-externals": "^1.6.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jmadan/manhattan"
  }
}
