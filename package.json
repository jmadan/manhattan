{
  "name": "manhattan",
  "version": "0.9.0",
  "description": "",
  "engines": {
    "node": "6.6.0"
  },
  "main": "./server/server.js",
  "scripts": {
    "clean": "rimraf build",
    "dev": "babel-node ./server/index.js",
    "test": "NODE_ENV=test mocha ./tests/* --compilers js:babel-core/register",
    "build": "babel ./server/index.js -d /build",
    "serve": "nodemon ./build/server/index.js",
    "webpack": "webpack --progress",
    "start:dev": "NODE_ENV=dev ./node_modules/.bin/babel-node ./server/index.js",
    "start": "NODE_ENV=prod && ./node_modules/.bin/babel-node ./server/index.js"
  },
  "keywords": [
    "node",
    "babel",
    "heroku"
  ],
  "author": "Jasdeep Madan",
  "license": "ISC",
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ]
  },
  "dependencies": {
    "babel-cli": "^6.16.0",
    "babel-polyfill": "^6.16.0",
    "bayes": "0.0.6",
    "body-parser": "^1.15.2",
    "cheerio": "^0.20.0",
    "express": "^4.14.0",
    "immutable": "^3.8.1",
    "mongoose": "^4.5.3",
    "morgan": "^1.7.0",
    "natural": "^0.4.0",
    "node-cron": "^1.1.1",
    "q": "^1.4.1",
    "request": "^2.73.0",
    "request-promise-native": "^1.0.3"
  },
  "devDependencies": {
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "chai-http": "^3.0.0",
    "clean-webpack-plugin": "^0.1.10",
    "nodemon": "^1.10.2",
    "rimraf": "^2.5.4",
    "sinon": "^1.17.6"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jmadan/manhattan"
  }
}
