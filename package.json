{
  "name": "manhattan",
  "version": "0.9.0",
  "description": "",
  "engines": {
    "node": "6.6.0"
  },
  "main": "./server/server.js",
  "scripts": {
    "clean": "rimraf build",
    "dev": "babel-node ./server/index.js",
    "test": "NODE_ENV=test mocha ./tests/* --compilers js:babel-core/register",
    "build": "babel ./server/index.js -d /build",
    "serve": "nodemon ./build/server/index.js",
    "webpack": "webpack --progress",
    "start:dev": "NODE_ENV=dev ./node_modules/.bin/babel-node ./server/index.js",
    "start": "NODE_ENV=prod && ./node_modules/.bin/babel-node ./server/index.js"
  },
  "keywords": [
    "node",
    "babel",
    "heroku"
  ],
  "author": "Jasdeep Madan",
  "license": "ISC",
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ]
  },
  "dependencies": {
    "babel-cli": "^6.24.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "bayes": "0.0.6",
    "body-parser": "^1.17.2",
    "cheerio": "^1.0.0-rc.1",
    "es6-promise": "^4.1.0",
    "express": "^4.15.3",
    "immutable": "^3.8.1",
    "isomorphic-fetch": "^2.2.1",
    "mongodb": "^2.2.28",
    "morgan": "^1.8.2",
    "natural": "^0.5.1",
    "node-cron": "^1.2.0"
  },
  "devDependencies": {
    "babel-loader": "^7.0.0",
    "chai": "^3.5.0",
    "chai-http": "^3.0.0",
    "clean-webpack-plugin": "^0.1.10",
    "mocha": "^3.4.2",
    "nodemon": "^1.10.2",
    "rimraf": "^2.5.4",
    "sinon": "^1.17.6",
    "webpack": "^2.6.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jmadan/manhattan"
  }
}
