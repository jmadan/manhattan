{
  "name": "manhattan",
  "version": "0.9.0",
  "description": "",
  "engines": {
    "node": "8.1.2"
  },
  "main": "./server/server.js",
  "scripts": {
    "clean": "rimraf build",
    "lint": "require-lint",
    "compile": "npm run clean && ./node_modules/.bin/babel server --out-dir build",
    "test": "npm run lint && NODE_ENV=test nyc mocha ./tests/* --compilers js:babel-core/register",
    "start": "npm run compile && ./node_modules/.bin/babel-node --max-old-space-size=4096 ./build/server.js"
  },
  "keywords": [
    "node",
    "babel"
  ],
  "author": "Jasdeep Madan",
  "license": "ISC",
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ],
      "stage-0",
      "stage-1"
    ]
  },
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "bayes": "0.0.6",
    "brain": "^0.7.0",
    "bunyan": "^1.8.12",
    "cheerio": "^1.0.0-rc.2",
    "cron": "^1.2.1",
    "feedparser": "^2.2.1",
    "htmlparser2": "^3.9.2",
    "mongodb": "^2.2.28",
    "natural": "^0.5.4",
    "nbayes": "^3.0.2",
    "node-cron": "^1.2.0",
    "parse5": "^3.0.2",
    "pg": "7.1.2",
    "redis": "^2.8.0",
    "request": "^2.81.0",
    "request-promise": "^4.2.2",
    "restify": "^6.0.0",
    "restify-cors-middleware": "^1.0.1",
    "synaptic": "^1.0.12",
    "textract": "^2.2.0",
    "url": "^0.11.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "chai": "^4.1.1",
    "eslint": "^4.7.2",
    "mocha": "^3.5.3",
    "mocha-as-promised": "^2.0.0",
    "nyc": "^11.1.0",
    "require-lint": "^1.3.0",
    "rimraf": "^2.5.4",
    "sinon": "^3.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jmadan/manhattan"
  }
}
